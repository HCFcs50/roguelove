    character: {fileID: 11400000, guid: 4672e976c526f4184853f303289586ff, type: 2}
    emotion: 0
    firstNameUsage: 0
    choices: []
  - sentence: "Normally I wouldn't really mind, but my dad's forcing me to stay inside
